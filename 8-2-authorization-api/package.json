    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-register": "^1.0.0",
    "typescript": "^5.1.3"
  }
