{
  "name": "purple-school-express",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "nodemon ./dist/main.js",
    "dev": "npx nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "build": "rm -rf ./dist && tsc",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.ts --coverage",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "clinic": "npm run build && clinic doctor --on-port 'autocannon -m POST localhost:8000/users/register' -- node dist/main.js",
    "generate": "npx prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
